<h1>Listing cart_lines</h1>

<table>
  <thead>
    <tr>
      <th>Cart</th>
      <th>Status</th>
      <th>Item</th>
      <th>Note</th>
      <th>Price in cents</th>
      <th>Qty</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cart_lines.each do |cart_line| %>
      <tr>
        <td><%= cart_line.cart_id %></td>
        <td><%= cart_line.status %></td>
        <td><%= cart_line.item_id %></td>
        <td><%= cart_line.note %></td>
        <td><%= cart_line.price_in_cents %></td>
        <td><%= cart_line.qty %></td>
        <td><%= link_to 'Show', cart_line %></td>
        <td><%= link_to 'Edit', edit_cart_line_path(cart_line) %></td>
        <td><%= link_to 'Destroy', cart_line, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cart line', new_cart_line_path %>
