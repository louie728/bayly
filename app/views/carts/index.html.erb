<h1>Listing carts</h1>

<table>
  <thead>
    <tr>
      <th>Customer</th>
      <th>Cusnumber</th>
      <th>Status</th>
      <th>User</th>
      <th>Descr</th>
      <th>Comments</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @carts.each do |cart| %>
      <tr>
        <td><%= cart.customer_id %></td>
        <td><%= cart.cusnumber %></td>
        <td><%= cart.status %></td>
        <td><%= cart.user_id %></td>
        <td><%= cart.descr %></td>
        <td><%= cart.comments %></td>
        <td><%= link_to 'Show', cart %></td>
        <td><%= link_to 'Edit', edit_cart_path(cart) %></td>
        <td><%= link_to 'Destroy', cart, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cart', new_cart_path %>
